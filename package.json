{
  "name": "cypress-explorer",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build": "yarn build:ui && yarn build:server",
    "build:ui": "vite build",
    "build:server": "yarn tsc --project plugin",
    "server": "ts-node plugin/server.ts",
    "lint": "prettier . --write"
  },
  "exports": {
    "./plugin": {
      "import": "./dist/server",
      "require": "./dist/server"
    },
    "./command": {
      "import": "./dist/cypress-explorer.es.js",
      "require": "./dist/cypress-explorer.umd.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lmiller1990/cypress-explorer.git"
  },
  "homepage": "https://github.com/lmiller1990/cypress-explorer",
  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@vitejs/plugin-vue": "^2.3.3",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "cypress": "^10.0.3",
    "debug": "^4.3.4",
    "dedent": "^0.7.0",
    "express": "^4.18.1",
    "prettier": "^2.6.2",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3",
    "vite": "^2.9.9",
    "vue-tsc": "^0.34.7"
  }
}
